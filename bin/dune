(executable
 (public_name whycode)
 (name main)
 (flags
  (:standard
   (:include linking-flags-fserv.sexp)))
 (libraries whycode why3 lsp linol linol-lwt uri lwt.unix)
 )

(rule
 (with-stdout-to
  linking-flags-fserv.sexp
  (run ./gen-linking-flags.sh %{env:LINKING_MODE=dynamic}
    %{ocaml-config:system})))
